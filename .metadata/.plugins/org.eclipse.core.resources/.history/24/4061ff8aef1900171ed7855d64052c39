package practiseDay_1;

import java.util.Scanner;

public class Tables 
{
	public static void main(String[] args) 
	{
		long n;
		
		Scanner sc = new Scanner(System.in);
		System.out.println("ENTER A NUMBER: ");
		n = sc.nextLong();
		
		{
			for(int i=1; i<=10; i++)
			{
				System.out.println(+n+" X "+i+" = "+(n*i));
			}
		}		
	}
}
