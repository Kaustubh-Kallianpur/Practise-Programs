{
	public static void main(String[] args) 
	{
		long n;
		
		Scanner sc = new Scanner(System.in);
		System.out.println("ENTER A NUMBER: ");
		n = sc.nextLong();
		
		public static void main(String[] args) 
		{
			for(int i=1; i<=10; i++)
			{
				System.out.println("Number: "+i+"\t Sqaure: "+(i*i));
			}
		}
	}
